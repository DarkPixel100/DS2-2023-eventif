<h1>Formulário de Inscrição</h1>
<form action="." method="post">
    {% csrf_token %}
    <p>
        <label for="{{ form.name.id_form_label }}">{{ form.name.label }}</label>
        {{ form.name.errors }}
        {{ form.name }}
    </p>
    <p>
        <label for="{{ form.cpf.id_form_label }}">{{ form.cpf.label }}</label>
        {{ form.cpf.errors }}
        {{ form.cpf }}
    </p>
    <p>
        <label for="{{ form.email.id_form_label }}">{{ form.email.label }}</label>
        {{ form.email.errors }}
        {{ form.email }}
    </p>
    <p>
        <label for="{{ form.phone.id_form_label }}">{{ form.phone.label }}</label>
        {{ form.phone.errors }}
        {{ form.phone }}
    </p>
    <input type="submit" value="Enviar!" />
</form>